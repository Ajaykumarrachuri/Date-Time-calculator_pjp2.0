pipeline { 
    agent any  
    stages { 
        stage('Build') { 
            steps { 
               echo 'Maven Test, Compile and Packaging using jenkinsfile .' 

            }
        }
        stage('Compile') { 
            steps { 
               call 'mvn compile'
            }
		}
		stage('Test') {
            steps {
                call 'mvn test'
            }
        }		
		stage('Install') {
            steps {
                call 'mvn install'
            }
        }
    }
}